    "allowSyntheticDefaultImports": true,
    "baseUrl": "src/",
    "paths": {
      "components/*": ["src/components/*"],
      "containers/*": ["src/containers/*"],
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "utils/*": ["src/utils/*"]
